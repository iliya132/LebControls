<UserControl x:Class="LebControls_NetFramework_.TimeSelector"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LebControls_NetFramework_"
             mc:Ignorable="d" 
             Height="60" d:DesignWidth="300">
    <Grid Name="OffGrid">
        <Grid Name="MainGrid" Margin="1" LostFocus="OffGrid_LostFocus" Focusable="True">
            <Grid.ColumnDefinitions >
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
        </Grid>
        <Border BorderThickness="1" BorderBrush="Black"/>
    </Grid>
</UserControl>
